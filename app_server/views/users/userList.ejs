<% layout('layouts/boilerplate') %>
<div class="container-fluid">
  <table class="table">
    <thead>
      <tr>
        <th>username</th>
        <th>email</th>
      </tr>
    </thead>
    <tbody>
      <% for(let user of foundUsers) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>
